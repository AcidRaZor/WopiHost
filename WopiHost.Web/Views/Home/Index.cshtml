@using System.Threading.Tasks
@using WopiHost.Web.Models
@{
	ViewData["Title"] = "Home Page";
}

<div class="row">
	<div class="col-md-3">
		<ul>
			@foreach (Child f in @Model.ChildContainers)
			{
				<li><a href="@f.Url">@f.Name</a></li>
			}
		</ul>
		<ul>
			@foreach (Child f in @Model.ChildFiles)
			{
				<li><a href="@f.Url">@f.Name</a></li>
			}
		</ul>
	</div>
</div>
