{
	"webroot": "wwwroot",
	"version": "1.0.0-*",

	"dependencies": {
		"Microsoft.AspNet.Mvc": "6.0.0-beta6",
		"Microsoft.AspNet.Server.IIS": "1.0.0-beta6",
		"Microsoft.AspNet.Hosting": "1.0.0-beta6",
		"Microsoft.AspNet.Server.WebListener": "1.0.0-beta6",
		"Microsoft.AspNet.Session": "1.0.0-beta6",
		"Microsoft.AspNet.StaticFiles": "1.0.0-beta6",
		"Microsoft.CobaltCore": "15.0.0",
		"Microsoft.Framework.ConfigurationModel": "1.0.0-beta5-11337",
		"Autofac": "4.0.0-beta6-97",
		"Autofac.Framework.DependencyInjection":  "4.0.0-beta6-97",
		"Microsoft.Framework.DependencyInjection": "1.0.0-beta6",
		"Microsoft.AspNet.Mvc.Core": "6.0.0-beta6"
	},

	"commands": {
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://wopihost:5000"
	},

	"frameworks": {
		"dnx451": {
			"dependencies": {
				"WopiDiscovery": "1.0.0-*",
				"WopiHost.Contracts": "1.0.0-*",
				"WopiFileSystemProvider": "1.0.0-*"
			},
			"frameworkAssemblies": {
				"System.Security": "4.0.0.0"
			}
		}
	},

	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	],
	"publishExclude": [
		"node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
	]
}
