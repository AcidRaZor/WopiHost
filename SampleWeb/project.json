{
	"webroot": "wwwroot",
	"version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta8",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-beta8",
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta8",
    "Microsoft.AspNet.Diagnostics.Entity": "7.0.0-beta8",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta8",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta8",
    "Microsoft.AspNet.Tooling.Razor": "1.0.0-beta8",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta8",
    "Microsoft.Framework.Configuration": "1.0.0-beta8",
    "Microsoft.Framework.Configuration.Abstractions": "1.0.0-beta8",
    "Microsoft.Framework.Logging": "1.0.0-beta8",
    "Microsoft.Framework.DependencyInjection": "1.0.0-beta8",
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-beta8",
    "Autofac": "4.0.0-beta7-130",
    "Autofac.Framework.DependencyInjection": "4.0.0-beta7-130",
    "Microsoft.Framework.Configuration.UserSecrets": "1.0.0-beta8"
  },

	"commands": {
		"web": "Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5000"		
	},

	"frameworks": {
		"dnx451": {
			"dependencies": {
				"WopiHost.Urls": "1.0.0-*",
				"WopiHost.Contracts": "1.0.0-*",
				"WopiFileSystemProvider": "1.0.0-*"
			}
		}
	},

	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	],
	"publishExclude": [
		"node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
	],
  "scripts": {
    "postrestore": [ "npm install", "bower install" ],
    "prepare": [ "gulp copy" ]
  },
  "userSecretsId": "aspnet5-SampleWeb-20150906061332"
}
